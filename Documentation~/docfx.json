{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "Runtime/**/*.cs"
          ]
        }
      ],
      "dest": "api",
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "LiveKit Unity SDK",
      "_appFooter": "LiveKit Client SDK for Unity WebGL",
      "_enableSearch": true
    },
    "sitemap": {
      "baseUrl": "https://livekit.github.io/client-sdk-unity-web"
    },
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "overwrite": [
      {
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}